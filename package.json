{
  "name": "switchbuffers",
  "displayName": "switchbuffers",
  "description": "Emacs like switch to buffer",
  "publisher": "innerout",
  "author": {
    "name": "George Xanthakis",
    "email": "kompiouterakias@hotmail.com"
  },
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "switchbuffers.switchFiles",
        "title": "Switch files"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "license": "SEE LICENSE",
  "bugs": {
    "url": "https://github.com/innerout/switchbuffers/issues",
    "email": "kompiouterakias@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/innerout/switchbuffers.git"
  },
  "homepage": "https://github.com/innerout/switchbuffers/blob/main/README.md",
  "dependencies": {
    "@vscode/vsce": "^2.29.0"
  }
}
